{
    "name": "ozdemir/aurora",
    "description": "Shopping Cart for Laravel",
    "keywords": [
        "ozdemir",
        "laravel",
        "laravel-cart",
        "shopping",
        "cart"
    ],
    "homepage": "https://github.com/ozdemir/laravel-cart",
    "license": "MIT",
    "authors": [
        {
            "name": "Yusuf Ã–zdemir",
            "email": "yusuf@ozdemir.be",
            "role": "Owner"
        }
    ],
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "pestphp/pest": "^2.0",
        "orchestra/testbench": "^v8.5",
        "nunomaduro/collision": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Ozdemir\\Aurora\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ozdemir\\Aurora\\Tests\\": "tests"
        }
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ozdemir\\Cart\\CartServiceProvider"
            ],
            "aliases": {
                "Cart": "Ozdemir\\Cart\\Facades\\Cart"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
